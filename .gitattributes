# Mark node modules and vendor directories as vendored
node_modules/** linguist-vendored
vendor/** linguist-vendored

# Mark common build / distribution output as generated
dist/** linguist-generated
build/** linguist-generated
public/dist/** linguist-generated
public/js/** linguist-generated
assets/** linguist-generated

# Minified / bundle files
*.min.js linguist-generated
*.bundle.js linguist-generated
*.map linguist-generated

# Lockfiles and package files that might bias analysis
package-lock.json linguist-vendored
yarn.lock linguist-vendored
